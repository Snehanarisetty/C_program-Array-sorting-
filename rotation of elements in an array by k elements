#include <stdio.h>
int main()
{
    int arr[] = {2,4,6,8,10};
    int n = sizeof(arr)/sizeof(arr[0]);
    int k = 3;
    int i,j;
    for (i = n-k,j = n-1;i < j;i++,j--) {
    int temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
    }
     for (i = 0,j = n- k -1;i < j;i++,j--) {
    int temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
     }
     for (i = 0,j = n - 1;i < j;i++,j--) {
    int temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
     }
    for(int i = 0;i < n;i++) {
        printf("%d",arr[i]);
    }
    return 0;
}
